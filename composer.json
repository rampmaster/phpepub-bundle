{
    "name": "rampmaster/phpepub-bundle",
    "description": "Symfony bundle for PHPePub library",
    "type": "symfony-bundle",
    "license": "LGPL-2.1",
    "keywords": ["symfony", "bundle", "epub", "ebook", "phpepub"],
    "authors": [
        {
            "name": "rampmaster",
            "homepage": "https://github.com/rampmaster"
        }
    ],
    "require": {
        "php": ">=8.2",
        "symfony/framework-bundle": "^5.4|^6.0|^7.0",
        "symfony/config": "^5.4|^6.0|^7.0",
        "symfony/dependency-injection": "^5.4|^6.0|^7.0",
        "symfony/http-kernel": "^5.4|^6.0|^7.0",
        "rampmaster/phpepub": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5|^10.0",
        "symfony/phpunit-bridge": "^5.4|^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Rampmaster\\PhpEpubBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rampmaster\\PhpEpubBundle\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
